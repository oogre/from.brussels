<div id="podcast">

</div>


<script type="text/javascript">
	window.addEventListener("load", function(){
		window.OOGRE = window.OOGRE || {};
		window.OOGRE.data = {
			_csrf : "<%= _csrf %>"
		};

		

		$.ajax({
			url : "/podcast/find",
			type : "get",
			data : {
				
			},
			dataType : "JSON"
		})
		.done(function(data){
			$("#podcast").append(window.OOGRE.templates("/podcast/list", {
				data : data
			}));	
		})
		
	});
</script>
