<div id="podcast">

</div>


<script type="text/javascript">
	window.addEventListener("load", function(){
		$.ajax({
			url : "/podcast/find",
			type : "get",
			dataType : "JSON"
		})
		.done(function(data){
			$("#podcast").append(window.OOGRE.templates("/podcast/list", {
				data : data
			}));	
		})
		
	});
</script>
