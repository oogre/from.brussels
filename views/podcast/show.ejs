<div id="podcast" class="pure-g">

</div>


<script type="text/javascript">
	window.addEventListener("load", function(){
		$.ajax({
			url : "/podcast/find",
			type : "get",
			data : {
				where : JSON.stringify({ id : "<%=  req.param('id') %>"})
			},
			dataType : "JSON"
		})
		.done(function(data){
			$("#podcast").append(window.OOGRE.templates("/podcast/form", data[0]));	
		})
		
	});
</script>
